@article{seibelDepthScaleMetabolic2007,
  title = {On the Depth and Scale of Metabolic Rate Variation: Scaling of Oxygen Consumption Rates and Enzymatic Activity in the {{Class Cephalopoda}}({{Mollusca}})},
  shorttitle = {On the Depth and Scale of Metabolic Rate Variation},
  author = {Seibel, Brad A.},
  date = {2007-01-01},
  journaltitle = {Journal of Experimental Biology},
  shortjournal = {Journal of Experimental Biology},
  volume = {210},
  number = {1},
  pages = {1--11},
  issn = {0022-0949},
  doi = {10.1242/jeb.02588},
  url = {https://doi.org/10.1242/jeb.02588},
  urldate = {2024-07-14},
  abstract = {Recent ecological theory depends, for predictive power, on the apparent similarity of metabolic rates within broad taxonomic or functional groups of organisms (e.g. invertebrates or ectotherms). Such metabolic commonality is challenged here, as I demonstrate more than 200-fold variation in metabolic rates independent of body mass and temperature in a single class of animals,the Cephalopoda, over seven orders of magnitude size range. I further demonstrate wide variation in the slopes of metabolic scaling curves. The observed variation in metabolism reflects differential selection among species for locomotory capacity rather than mass or temperature constraints. Such selection is highest among epipelagic squids (Lolignidae and Ommastrephidae)that, as adults, have temperature-corrected metabolic rates higher than mammals of similar size.},
  file = {/home/michael-schmutzer/snap/zotero-snap/common/Zotero/storage/8JME4N7V/Seibel - 2007 - On the depth and scale of metabolic rate variation.pdf;/home/michael-schmutzer/snap/zotero-snap/common/Zotero/storage/YJDQ24AN/On-the-depth-and-scale-of-metabolic-rate-variation.html}
}

