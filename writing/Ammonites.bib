@article{landmanConceptHeteromorphAmmonoids2021,
  title = {The Concept of ‘Heteromorph Ammonoids’},
  author = {Landman, Neil H. and Machalski, Marcin and Whalen, Christopher D.},
  date = {2021-12},
  journaltitle = {Lethaia},
  volume = {54},
  number = {5},
  pages = {595--602},
  publisher = {Scandinavian University Press},
  issn = {0024-1164},
  doi = {10.1111/let.12443},
  url = {https://www.scup.com/doi/10.1111/let.12443},
  urldate = {2025-09-25},
  abstract = {‘Heteromorph ammonoids’ encompass all ammonoid species whose shapes do not conform to a closely coiled planispiral shell. The term is useful as a broad description for such ammonoids. However, as a concept, ‘heteromorph ammonoids’ no longer has any scientific value or explanatory power. Although such ammonoids have traditionally been considered aberrant forms, they represent instead an integral part of the evolutionary history of the Ammonoidea. ‘Heteromorph ammonoids’, as a whole, are a polyphyletic group, consisting of a heterogeneous mixture of taxa without any phylogenetic, morphological or ecological coherence. Their treatment as a single entity risks conflating convergences and phylogenetic affinities. It also vastly oversimplifies the stunning array of morphologies and ecological niches occupied by these animals. Investigation into the uncoiling (and recoiling) of ammonoids is a legitimate and worthwhile enterprise, especially in view of the realization that this phenomenon occurred several times in the history of the Ammonoidea. However, few insights can be gained by treating ‘heteromorph ammonoids’ as a single entity. Studies of such ammonoids should focus on monophyletic groups within a well‐constrained phylogenetic and stratigraphical framework to yield meaningful results. Keywords Ammonoids Ancyloceratina heteromorphs morphotype Scaphitidae},
  keywords = {Ammonoids,Ancyloceratina,heteromorphs,morphotype,Scaphitidae},
  file = {/home/michael-schmutzer/snap/zotero-snap/common/Zotero/storage/GN385X9U/Landman et al. - 2021 - The concept of ‘heteromorph ammonoids’.pdf}
}

