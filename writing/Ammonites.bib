@article{brayardGoodGenesGood2009,
  title = {Good {{Genes}} and {{Good Luck}}: {{Ammonoid Diversity}} and the {{End-Permian Mass Extinction}}},
  shorttitle = {Good {{Genes}} and {{Good Luck}}},
  author = {Brayard, Arnaud and Escarguel, Gilles and Bucher, Hugo and Monnet, Claude and Brühwiler, Thomas and Goudemand, Nicolas and Galfetti, Thomas and Guex, Jean},
  date = {2009-08-28},
  journaltitle = {Science},
  volume = {325},
  number = {5944},
  pages = {1118--1121},
  publisher = {American Association for the Advancement of Science},
  doi = {10.1126/science.1174638},
  url = {https://www.science.org/doi/full/10.1126/science.1174638},
  urldate = {2025-05-09},
  abstract = {The end-Permian mass extinction removed more than 80\% of marine genera. Ammonoid cephalopods were among the organisms most affected by this crisis. The analysis of a global diversity data set of ammonoid genera covering about 106 million years centered on the Permian-Triassic boundary (PTB) shows that Triassic ammonoids actually reached levels of diversity higher than in the Permian less than 2 million years after the PTB. The data favor a hierarchical rather than logistic model of diversification coupled with a niche incumbency hypothesis. This explosive and nondelayed diversification contrasts with the slow and delayed character of the Triassic biotic recovery as currently illustrated for other, mainly benthic groups such as bivalves and gastropods.},
  file = {/home/michael-schmutzer/snap/zotero-snap/common/Zotero/storage/C3JL6XDU/Brayard et al. - 2009 - Good Genes and Good Luck Ammonoid Diversity and the End-Permian Mass Extinction.pdf}
