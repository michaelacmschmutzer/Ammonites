@article{baudouinRevisionEarlyCretaceous2016,
  title = {Revision of the {{Early Cretaceous}} Genera {{Heminautilus Spath}}, 1927, and {{Josanautilus Martínez}} \& {{Grauges}}, 2006 ({{Nautilida}}, {{Cenoceratidae}})},
  author = {Baudouin, Cyril and Delanoy, Gérard and Moreno-Bedmar, Josep Anton and Pictet, Antoine and Vermeulen, Jean and Conte, Gabriel and Gonnet, Roland and Boselli, Patrick and Boselli, Marc},
