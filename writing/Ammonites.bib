  journaltitle = {Cretaceous Research},
  shortjournal = {Cretaceous Research},
  volume = {25},
  number = {3},
  pages = {313--328},
  issn = {0195-6671},
  doi = {10.1016/j.cretres.2004.01.002},
  url = {https://www.sciencedirect.com/science/article/pii/S0195667104000254},
  urldate = {2025-09-16},
  abstract = {Ammonites are described for the first time from the terminal Maastrichtian in the Cretaceous–Paleogene (K/P) Global Stratotype Section and Point at El Kef, Tunisia. A rich assemblage, consisting of at least 17 taxa, occurs in the interval from 7 to 2m below the K/P boundary. They co-occur with a diverse macrofauna, which also includes rudists. All fossils are pyritic, and the ammonites are all nuclei, mostly {$<$}20mm in diameter. The dominance of Indoscaphites suggests open connections with southern India along the southern Tethys. No ammonites have been recovered from the 2-m interval below the K/P boundary in the Tunisian basin, although other macrofossil groups, including some originally aragonitic bivalve taxa, persist up to the boundary and are also present in the basal Paleogene.},
  keywords = {Ammonites,El Kef GSSP,Terminal Maastrichtian,Tunisia},
  file = {/home/michael-schmutzer/snap/zotero-snap/common/Zotero/storage/SAIRPXUE/Goolaerts et al. - 2004 - Terminal Maastrichtian ammonites from the Cretaceous–Paleogene Global Stratotype Section and Point,.pdf;/home/michael-schmutzer/snap/zotero-snap/common/Zotero/storage/2E5YX228/S0195667104000254.html}
}

